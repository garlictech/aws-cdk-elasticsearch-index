{
  "name": "@casechek/aws-cdk-elasticsearch-index",
  "version": "1.0.0-alpha.8",
  "description": "Elasticsearch Index Custom Resource for AWS CDK",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "concurrently \"webpack\" \"tsc --project tsconfig.lib.json\"",
    "build:prod": "concurrently \"webpack --mode production\" \"tsc --project tsconfig.lib.json\"",
    "watch": "concurrently \"webpack --watch\" \"tsc --project tsconfig.lib.json -w\"",
    "test": "jest",
    "fix": "gts fix",
    "commit": "git-cz",
    "cucumber": "cucumber-js",
    "webpack": "webpack",
    "check": "gts check",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.26.0",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@elastic/elasticsearch": "7.6.0",
    "@semantic-release/changelog": "5.0.0",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.0.4",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@types/aws-lambda": "8.10.46",
    "@types/aws-sdk": "2.7.0",
    "@types/axios": "0.14.0",
    "@types/chai": "4.2.10",
    "@types/cucumber": "6.0.1",
    "@types/deep-equal": "1.0.1",
    "@types/jest": "25.1.4",
    "@types/jsonschema": "1.1.1",
    "@types/memory-fs": "0.3.2",
    "@types/mock-fs": "4.10.0",
    "@types/node": "10.17.16",
    "@types/webpack": "4.41.7",
    "aws-cdk": "1.26.0",
    "aws-xray-sdk-core": "2.5.0",
    "axios": "0.19.2",
    "chai": "4.2.0",
    "concurrently": "5.1.0",
    "cucumber": "5.1.0",
    "git-cz": "4.3.1",
    "gts": "1.1.2",
    "husky": "4.2.3",
    "jest": "25.1.0",
    "jsonschema": "1.2.5",
    "semantic-release": "17.0.4",
    "semantic-release-contributors": "1.0.0",
    "ts-jest": "25.2.1",
    "ts-loader": "6.2.1",
    "ts-node": "8.6.2",
    "typescript": "3.8.3",
    "webpack": "4.42.0",
    "webpack-cli": "3.3.11"
  },
  "peerDependencies": {
    "@aws-cdk/core": "^1.26.0"
  },
  "dependencies": {
    "@aws-cdk/aws-cloudformation": "^1.26.0",
    "@aws-cdk/aws-ec2": "^1.26.0",
    "@aws-cdk/aws-elasticsearch": "^1.26.0",
    "@aws-cdk/aws-iam": "^1.26.0",
    "@aws-cdk/aws-lambda": "^1.26.0",
    "@aws-cdk/core": "^1.26.0",
    "@aws-cdk/custom-resources": "^1.26.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E  HUSKY_GIT_PARAMS",
      "pre-commit": "npm run check && npm test -- --coverage",
      "post-merge": "echo \"$(git diff-tree -r --name-only --no-commit-id ORIG_HEAD HEAD)\" | grep --quiet '^package-lock.json$' && npm install",
      "post-checkout": "echo \"$(git diff-tree -r --name-only --no-commit-id ORIG_HEAD HEAD)\" | grep --quiet '^package-lock.json$' && npm install"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/incompass/aws-cdk-elasticsearch-index"
  },
  "files": [
    "dist"
  ]
}
